class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        count=0
        sathwik=[]
        P=[]
        for i in range(len(s)):
            Praneeth=s[i:i+1]
            if Praneeth not in sathwik:
                count=count+1
                sathwik.append(Praneeth)
            else:
                P.append(count)
                K=sathwik.index(Praneeth)
                sathwik=sathwik[K+1:]
                sathwik.append(Praneeth)
                count=len(sathwik)
        P.append(count)
        if P:
            return max(P)
        else:
            return len(s)



