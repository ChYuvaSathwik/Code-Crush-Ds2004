class Solution:
    def relativeSortArray(self, arr1: List[int], arr2: List[int]) -> List[int]:
        sathwik=[]
        Praneeth=[]
        for i in range(len(arr2)):
            for j in range(len(arr1)):
                if arr2[i]==arr1[j]:
                    sathwik.append(arr2[i])
        for q in range(len(arr1)):
            if arr1[q] not in sathwik:
                Praneeth.append(arr1[q])
        Praneeth.sort()
        for k in range(len(Praneeth)):
            sathwik.append(Praneeth[k])
        return sathwik
            

