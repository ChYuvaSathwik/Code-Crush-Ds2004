class Solution:
    def countConsistentStrings(self, allowed: str, words: List[str]) -> int:
        PO=[]
        for u in allowed:
            PO.append(u)
        print(PO)
        Count=0
        for i in words:
            M=True
            for k in i:
                if k in PO:
                    M=True
                else:
                    M=False
                    break
            if M:
                Count=Count+1
        return Count
                    
        
