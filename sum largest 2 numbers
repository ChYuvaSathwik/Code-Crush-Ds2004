def largestPair(sathwik):
    balu=len(sathwik)
    th=float("-inf")
    for i in range(balu):
        for j in range(balu):
            if i!=j:
                cp=sathwik[i]+sathwik[j]
                th=max(cp,th)
    return th
sathwik=list(map(int,input().split()))
print(largestPair(sathwik)


